    "grunt-cli": "^1.2.0",
    "grunt-contrib-cssmin": "^2.2.0",
    "grunt-sass": "^2.0.0",
    "load-grunt-tasks": "^3.5.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "devtool": "^2.3.0",
    "istanbul": "^0.4.5",
